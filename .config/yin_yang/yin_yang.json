{
    "version": 3.3,
    "running": false,
    "dark_mode": false,
    "mode": "sunset to sunrise",
    "coordinates": [
        50.0,
        35.0
    ],
    "update_location": false,
    "update_interval": 60,
    "times": [
        "07:00",
        "20:00"
    ],
    "boot_offset": 10,
    "plugins": {
        "system": {
            "enabled": true,
            "light_theme": "Catppuccin-Latte-Sapphire",
            "dark_theme": "Catppuccin-Mocha-Sapphire"
        },
        "colors": {
            "enabled": true,
            "light_theme": "CatppuccinLatteSapphire",
            "dark_theme": "CatppuccinMochaSapphire"
        },
        "gtk": {
            "enabled": false,
            "light_theme": "Breeze",
            "dark_theme": "Breeze"
        },
        "icons": {
            "enabled": false,
            "light_theme": "",
            "dark_theme": ""
        },
        "kvantum": {
            "enabled": false,
            "light_theme": "KvFlatLight",
            "dark_theme": "KvFlat"
        },
        "wallpaper": {
            "enabled": false,
            "light_theme": null,
            "dark_theme": null
        },
        "firefox": {
            "enabled": false,
            "light_theme": "firefox-compact-light@mozilla.org",
            "dark_theme": "firefox-compact-dark@mozilla.org"
        },
        "vs code": {
            "enabled": true,
            "light_theme": "Catppuccin Latte",
            "dark_theme": "Catppuccin Mocha"
        },
        "onlyoffice": {
            "enabled": false,
            "light_theme": "theme-light",
            "dark_theme": "theme-dark"
        },
        "okular": {
            "enabled": false,
            "light_theme": "",
            "dark_theme": "InvertLightness"
        },
        "konsole": {
            "enabled": false,
            "light_theme": "BlackOnWhite",
            "dark_theme": "Breeze"
        },
        "custom": {
            "enabled": true,
            "light_theme": "\"$HOME/.config/yin_yang/toggle.sh\" light",
            "dark_theme": "\"$HOME/.config/yin_yang/toggle.sh\" dark"
        },
        "notification": {
            "enabled": false,
            "light_theme": "Day",
            "dark_theme": "Night"
        }
    }
}